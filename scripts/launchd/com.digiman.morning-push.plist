<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.digiman.morning-push</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/mac/Projects/digiman/venv/bin/python</string>
        <string>/Users/mac/Projects/digiman/scripts/morning_push.py</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Users/mac/Projects/digiman</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>8</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>
    <key>StandardOutPath</key>
    <string>/tmp/digiman-morning-push.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/digiman-morning-push.err</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin</string>
    </dict>
</dict>
</plist>
